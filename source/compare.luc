module compare (
    input z,
    input v,
    input n,
    input alufn_signal[6],
    output out[16] 
  ) {
  
  always {
    case (alufn_signal[2:1]) {
      default: out = 16b0;
      b1001: out = 16b0 + z;
      b1010: out = 16b0 + (n ^ v);
      b1011: out = 16b0 + (z | (n ^ v));
    }
  }
}